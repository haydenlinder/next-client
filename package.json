{
  "name": "next-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run tailwind:build && next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind:build": "touch ./styles/build.css && tailwindcss -o ./styles/build.css --minify",
    "tailwind:watch": "touch ./styles/build.css && tailwindcss -i ./styles/input.css -o ./styles/build.css --watch",
    "generate": "graphql-codegen --config codegen.js",
    "pretty": "prettier --write ./**/*"
  },
  "dependencies": {
    "@apollo/client": "^3.6.2",
    "@sendgrid/mail": "^7.7.0",
    "aws-sdk": "^2.1159.0",
    "bcrypt": "^5.0.1",
    "cookie": "^0.5.0",
    "crypto": "^1.0.1",
    "graphql": "^16.5.0",
    "jsonwebtoken": "^8.5.1",
    "minio": "^7.0.28",
    "multer": "^1.4.5-lts.1",
    "multer-sharp-s3": "^0.2.5",
    "next": "12.1.6",
    "next-connect": "^0.12.2",
    "prettier": "^2.6.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-dropzone": "^14.2.1",
    "react-markdown": "^8.0.3",
    "react-modal": "^3.15.1",
    "react-syntax-highlighter": "^15.5.0",
    "schema-dts": "^1.1.0",
    "sharp": "^0.30.7",
    "tailwindcss": "^3.0.24",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-codegen/typescript-operations": "^2.4.0",
    "@graphql-codegen/typescript-react-apollo": "^3.2.14",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.5.1",
    "@types/minio": "^7.0.13",
    "@types/multer": "^1.4.7",
    "@types/node": "17.0.32",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "@types/react-modal": "^3.13.1",
    "@types/react-syntax-highlighter": "^15.5.5",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "typescript": "4.6.4"
  }
}
